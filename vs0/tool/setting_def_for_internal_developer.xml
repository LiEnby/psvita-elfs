<registry target="psp2" version = "2">

	<!-- Development Environment Settings : 開発環境設定 -->
	<group eng="Development Environment Settings" jpn="開発環境設定">
		<desc eng="Adjusts settings for the development environment." jpn="開発環境全般に関する設定です。"/>

		<!-- Setting for Internal Developers : 内部開発者向け設定 -->
		<entry key="DEVENV/HOST/host_internal" eng="Setting for Internal Developers" jpn="内部開発者向け設定"
				internal="true" dependskey="internalhost" registrytype="integer" registrybytesize="4"  forcerefresh="true">
			<desc eng="If you set this option to [On], the settings for internal developers will be displayed.&#xa;This setting will be exclusively executed."
				  jpn="[入]にすると、内部開発者向け設定を表示します。&#xa;この設定は単独で実行されます。"/>
			<applyimmediate eng="This setting will be exclusively executed. Any already set settings will be ignored.&#xa;Are you sure you want to continue?"
					 jpn="この設定は単独で実行されます。すでに編集された他の設定値は無視されます。&#xa;本当によろしいですか？"/>
			<selection>
				<option value="0" eng="Off" jpn="切"/>
				<option value="1" eng="On" jpn="入"/>
			</selection>
		</entry>

		<!-- System Name : 本体名称 -->
		<entry key="DEVENV/HOST/devkitname" eng="System Name" jpn="本体名称"
				registrytype="string" registrybytesize="128" >
			<desc eng="Sets the name for this system (DevKit). You can use the name to distinguish multiple DevKits on a network."
				  jpn="本機(開発機)の名前を設定します。ネットワーク上で複数の開発機を区別するときなどに使われます。"/>
			<textual minlen="0" maxlen="31" encoding="utf8"/>
		</entry>

		<!-- HDMI Video Output Settings : HDMI 映像出力設定 -->
		<entry key="DEVENV/TOOL/hdmi_video_mode" eng="HDMI Video Output Settings" jpn="HDMI 映像出力設定"
				registrytype="integer" registrybytesize="4" norebootneeded="true">
			<desc eng="Sets the resolution and scaling type of the HDMI video output."
				  jpn="HDMI 映像出力の解像度と拡大/縮小方法を設定します。"/>
			<selection>
				<option value="8" eng="Resolution : 480p, Scaling : Full" jpn="解像度 : 480p, 拡大/縮小 : フル"/>
				<option value="32" eng="Resolution : 720p, Scaling : Full" jpn="解像度 : 720p, 拡大/縮小 : フル"/>
				<option value="65568" eng="Resolution : 720p, Scaling : Real" jpn="解像度 : 720p, 拡大/縮小 : リアル"/>
			</selection>
		</entry>

	</group>

	<!-- Network : ネットワーク -->
	<group eng="Network" jpn="ネットワーク">
		<desc eng="" jpn=""/>

		<!-- Flight Mode : 機内モード -->
		<entry key="CONFIG/SYSTEM/flight_mode" eng="Flight Mode" jpn="機内モード"
				registrytype="integer" registrybytesize="4" dependson="internalhost" conditionmin="1" conditionmax="1">
			<desc eng="Sets whether to enable the flight mode. If you set this option to [On], All network features of the VITA system are disabled."
				  jpn="機内モードを有効にするかどうかを設定します。[入]にすると、「VITA」のすべての通信機能が使用できません。"/>
			<selection>
				<option value="0" eng="Off" jpn="切"/>
				<option value="1" eng="On" jpn="入"/>
			</selection>
		</entry>

		<!-- Wi-Fi Settings : Wi-Fi設定 -->
		<group eng="Wi-Fi Settings" jpn="Wi-Fi設定">
			<desc eng="Adjusts settings for the Wi-Fi connection." jpn="Wi-Fi接続に関する設定です。"/>

			<!-- In case of "Setting for Internal Developers = On" : 「開発環境設定 = 入」の場合 -->
			<set dependson="internalhost" conditionmin="1" conditionmax="1">

			<!-- Wi-Fi -->
			<entry key="CONFIG/NET/wifi_flag" eng="Wi-Fi"
					registrytype="integer" registrybytesize="4">
				<desc eng="Sets whether to enable the Wi-Fi feature."
					  jpn="Wi-Fi機能を有効にするかどうかを設定します。"/>
				<selection>
					<option value="0" eng="Off" jpn="切"/>
					<option value="1" eng="On" jpn="入"/>
				</selection>
			</entry>

			<!-- Wi-Fi Ad Hoc Mode Channel Number : Wi-Fiアドホックモードチャンネル番号 -->
			<entry key="CONFIG/NET/adhoc_channel" eng="Wi-Fi Ad Hoc Mode Channel Number" jpn="Wi-Fiアドホックモードチャンネル番号"
					registrytype="integer" registrybytesize="4">
				<desc eng="Sets the channel for Wi-Fi ad hoc mode. "
					  jpn="Wi-Fiアドホックモードで使用するチャンネル番号を設定します。"/>
				<selection>
					<option value="0" eng="Automatic" jpn="自動"/>
					<option value="1" eng="1 ch" jpn="1ch"/>
					<option value="6" eng="6 ch" jpn="6ch"/>
					<option value="11" eng="11 ch" jpn="11ch"/>
				</selection>
			</entry>

			</set>

			<!-- Wi-Fi Ad Hoc Mode SSID : Wi-FiアドホックモードSSID -->
			<entry key="CONFIG/NET/adhoc_ssid" eng="Wi-Fi Ad Hoc Mode SSID" jpn="Wi-FiアドホックモードSSID"
					registrytype="string" registrybytesize="33" dependson="internalhost" conditionmin="1" conditionmax="1">
				<desc eng="Sets the SSID for Wi-Fi ad hoc mode."
					  jpn="Wi-Fiアドホックモードで使用するSSIDを設定します。"/>
				<textual minlen="0" maxlen="32" encoding="ascii"/>
			</entry>

			<!-- Connection #01-30 : Wi-Fiインターネット接続 #01-30 -->
			<config keymin="1" keymax="30" keysigfig="2" validkey="CONFIG/NET/#/COMMON/conf_flag"
					validkeyvalue="1" validkeyregistrytype="integer" eng="Wi-Fi Internet Connection" jpn="Wi-Fiインターネット接続">
				<desc eng="Adjusts settings for the Wi-Fi Internet connection."
					  jpn="Wi-Fiインターネット接続に関する設定です。"/>

				<!-- Connection Name : 接続名 -->
				<entry key="CONFIG/NET/#/COMMON/conf_name" eng="Connection Name" jpn="接続名"
						registrytype="string" registrybytesize="65" default="">
					<desc eng="Sets the name of the Internet connection setting."
						  jpn="インターネット接続の接続名を設定します。"/>
					<textual minlen="1" maxlen="16" encoding="utf8"/>
				</entry>

				<!-- In case of "Setting for Internal Developers = On" : 「開発環境設定 = 入」の場合 -->
				<set dependson="internalhost" conditionmin="1" conditionmax="1">

				<!-- Automatic Connect : 自動接続 -->
				<entry key="CONFIG/NET/#/COMMON/enable_auto_connect" eng="Auto Connect" jpn="自動接続"
						registrytype="integer" registrybytesize="4" default="1">
					<desc eng="If you set this option to [On], automatic connection is made when a connection is found."
						  jpn="[入]にすると、この接続が見つかったときに自動で接続します。"/>
					<selection>
						<option value="0" eng="Off" jpn="切"/>
						<option value="1" eng="On" jpn="入"/>
					</selection>
				</entry>

				</set>

				<!-- MTU -->
				<entry key="CONFIG/NET/#/COMMON/mtu" eng="MTU"
						registrytype="integer" registrybytesize="4" default="0">
					<desc eng="Sets the MTU value to use when transmitting data. Set a number from the ranges 576-1500. Use 0 for automatic setup. "
						  jpn="データ通信時に使用するMTUの値を設定します。576から1500の値を設定します。自動設定にするには0を入力してください。"/>
					<intvalue minvalue="576" maxvalue="1500" rangeexception="0"/>
				</entry>

				<!-- Networking Device : 接続デバイス -->
				<entry key="CONFIG/NET/#/COMMON/device" eng="Networking Device" jpn="接続デバイス"
						registrytype="integer" registrybytesize="4" default="0">
					<desc eng="Sets the device for the Internet connection."
						  jpn="インターネット接続に使用するデバイスを設定します。"/>
					<selection>
						<option value="0" eng="Wireless" jpn="無線"/>
						<option value="1" eng="Wired Connection" jpn="有線"/>
					</selection>
				</entry>

				<!-- In case of "Setting for Internal Developers = On" : 「開発環境設定 = 入」の場合 -->
				<set dependson="internalhost" conditionmin="1" conditionmax="1">

				<!-- SSID -->
				<entry key="CONFIG/NET/#/WIFI/ssid" eng="SSID"
						registrytype="string" registrybytesize="33" default="">
					<desc eng="Sets the SSID of the access point."
						  jpn="アクセスポイントのSSIDを設定します。"/>
					<textual minlen="1" maxlen="32" encoding="ascii"/>
				</entry>

				<!-- Wi-Fi Security : Wi-Fiセキュリティ -->
				<entry key="CONFIG/NET/#/WIFI/wifi_security" eng="Wi-Fi Security" jpn="Wi-Fiセキュリティ"
						registrytype="integer" registrybytesize="4" dependskey="wifisecurity"  default="0">
					<desc eng="Sets the security type of the access point."
						  jpn="アクセスポイントのセキュリティー種別を設定します。"/>
					<selection>
						<option value="0" eng="None" jpn="なし"/>
						<option value="1" eng="WEP"/>
						<option value="2" eng="WPA-PSK/WPA2-PSK"/>
					</selection>
				</entry>

				<!-- WEP Key : WEPキー -->
				<entry key="CONFIG/NET/#/WIFI/wep_key" eng="WEP Key" jpn="WEPキー"
						dependson="wifisecurity" conditionmin="1" conditionmax="1" registrytype="string" registrybytesize="27" default="">
					<desc eng="Sets the WEP encryption key. Use 10 or 26 characters for hexadecimal (0x prefix not required), or 5 or 13 characters for ASCII."
						  jpn="WEP暗号化キーを設定します。16進数(10文字か26文字、先頭に0xは不要)か、ASCII文字(5文字か13文字)で入力してください。"/>
					<wepkey/>
				</entry>

				<!-- WPA Key : WPAキー -->
				<entry key="CONFIG/NET/#/WIFI/wpa_key" eng="WPA Key"  jpn="WPAキー"
						dependson="wifisecurity" conditionmin="2" conditionmax="2" registrytype="string" registrybytesize="65" default="">
					<desc eng="Sets the WPA encryption key. Use 64 characters for hexadecimal (0x prefix not required), or from 8 to 63 characters for ASCII."
						  jpn="WPA暗号化キーを設定します。16進数(64文字、先頭に0xは不要)か、ASCII文字(8～63文字)で入力してください。"/>
					<wpakey/>
				</entry>

				</set>

				<!-- IP Address Setting : IPアドレス設定 -->
				<entry key="CONFIG/NET/#/IP/ip_config" eng="IP Address Setting" jpn="IPアドレス設定"
						registrytype="integer" registrybytesize="4" dependskey="ipconfig" default="0">
					<desc eng="Select how to set the IP address."
						  jpn="IPアドレスの設定方法を選択します。"/>
					<selection>
						<option value="0" eng="Automatic" jpn="自動取得"/>
						<option value="1" eng="Manual" jpn="手動"/>
						<option value="2" eng="PPPoE" jpn="PPPoE"/>
					</selection>
				</entry>

				<!-- In case of "IP Address Setting = Automatic" : 「IPアドレス設定 = 自動取得」の場合 -->
				<set dependson="ipconfig" conditionmin="0" conditionmax="0">

					<!-- DHCP Host Name : DHCPホスト名 -->
					<entry key="CONFIG/NET/#/IP/dhcp_hostname" eng="DHCP Host Name" jpn="DHCPホスト名"
							registrytype="string" registrybytesize="256" default="">
						<desc eng="Sets the DHCP host name. In most cases this setting does not need to be changed."
							  jpn="DHCPホスト名を設定します。通常は設定する必要はありません。"/>
						<textual minlen="0" maxlen="255" encoding="ascii"/>
					</entry>

					<!-- DNS Setting : DNS設定 -->
					<entry key="CONFIG/NET/#/IP/dns_flag" eng="DNS Setting" jpn="DNS設定"
							registrytype="integer" registrybytesize="4" dependskey="dnsflag1" default="0">
						<desc eng="Select how to set the DNS server addresses."
							  jpn="DNSサーバーのアドレスの設定方法を選択します。"/>
						<selection>
							<option value="0" eng="Automatic" jpn="自動取得"/>
							<option value="1" eng="Manual" jpn="手動"/>
						</selection>
					</entry>

					<!-- In case of "DNS Setting = Manual" : 「DNS設定 = 手動」の場合 -->
					<set dependson="dnsflag1" conditionmin="1" conditionmax="1">

						<!-- Primary DNS : プライマリーDNS -->
						<entry key="CONFIG/NET/#/IP/primary_dns" eng="Primary DNS" jpn="プライマリーDNS"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=""
								  jpn=""/>
							<ipaddress/>
						</entry>

						<!-- Secondary DNS : セカンダリーDNS -->
						<entry key="CONFIG/NET/#/IP/secondary_dns" eng="Secondary DNS" jpn="セカンダリーDNS"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=""
								  jpn=""/>
							<ipaddress/>
						</entry>

					</set>
				</set>

				<!-- In case of "IP Address Setting = Manual" : 「IPアドレス設定 = 手動」の場合 -->
				<set dependson="ipconfig" conditionmin="1" conditionmax="1">

					<!-- IP Address : IPアドレス -->
					<entry key="CONFIG/NET/#/IP/ip_address" eng="IP Address" jpn="IPアドレス"
							registrytype="string" registrybytesize="16" default="">
						<desc eng=""
							  jpn=""/>
						<ipaddress/>
					</entry>

					<!-- Subnet Mask : サブネットマスク -->
					<entry key="CONFIG/NET/#/IP/netmask" eng="Subnet Mask" jpn="サブネットマスク"
							registrytype="string" registrybytesize="16" default="">
						<desc eng=""
							  jpn=""/>
						<ipaddress/>
					</entry>

					<!-- Default Router : デフォルトルーター -->
					<entry key="CONFIG/NET/#/IP/default_route" eng="Default Router" jpn="デフォルトルーター"
							registrytype="string" registrybytesize="16" default="">
						<desc eng=""
							  jpn=""/>
						<ipaddress/>
					</entry>

					<!-- Primary DNS : プライマリーDNS -->
					<entry key="CONFIG/NET/#/IP/primary_dns" eng="Primary DNS" jpn="プライマリーDNS"
							registrytype="string" registrybytesize="16" default="">
						<desc eng=""
							  jpn=""/>
						<ipaddress/>
					</entry>

					<!-- Secondary DNS : セカンダリーDNS -->
					<entry key="CONFIG/NET/#/IP/secondary_dns" eng="Secondary DNS" jpn="セカンダリーDNS"
							registrytype="string" registrybytesize="16" default="">
						<desc eng=""
							  jpn=""/>
						<ipaddress/>
					</entry>

				</set>

				<!-- In case of "IP Address Setting = PPPoE" : 「IPアドレス設定 = PPPoE」の場合 -->
				<set dependson="ipconfig" conditionmin="2" conditionmax="2">

					<!-- PPPoE User Name : PPPoEユーザーID -->
					<entry key="CONFIG/NET/#/IP/auth_name" eng="PPPoE User Name" jpn="PPPoEユーザーID"
							registrytype="string" registrybytesize="128" default="">
						<desc eng=""
							  jpn=""/>
						<textual minlen="1" maxlen="127" encoding="ascii"/>
					</entry>

					<!-- PPPoE Password : PPPoEパスワード -->
					<entry key="CONFIG/NET/#/IP/auth_key" eng="PPPoE Password" jpn="PPPoEパスワード"
							registrytype="string" registrybytesize="128" default="">
						<desc eng=""
							  jpn=""/>
						<password minlen="0" maxlen="127"/>
					</entry>

					<!-- DNS Setting : DNS設定 -->
					<entry key="CONFIG/NET/#/IP/dns_flag" eng="DNS Setting" jpn="DNS設定"
							registrytype="integer" registrybytesize="4" dependskey="dnsflag2" default="0">
						<desc eng="Select how to set the DNS server addresses."
							  jpn="DNSサーバーのアドレスの設定方法を選択します。"/>
						<selection>
							<option value="0" eng="Automatic" jpn="自動取得"/>
							<option value="1" eng="Manual" jpn="手動"/>
						</selection>
					</entry>

					<!-- In case of "DNS Setting = Manual" : 「DNS設定 = 手動」の場合 -->
					<set dependson="dnsflag2" conditionmin="1" conditionmax="1">

						<!-- Primary DNS : プライマリーDNS -->
						<entry key="CONFIG/NET/#/IP/primary_dns" eng="Primary DNS" jpn="プライマリーDNS"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=""
								  jpn=""/>
							<ipaddress/>
						</entry>

						<!-- Secondary DNS : セカンダリーDNS -->
						<entry key="CONFIG/NET/#/IP/secondary_dns" eng="Secondary DNS" jpn="セカンダリーDNS"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=""
								  jpn=""/>
							<ipaddress/>
						</entry>

					</set>
				</set>

				<!-- Proxy Server : プロキシーサーバー -->
				<entry key="CONFIG/NET/#/APP/http_proxy_flag" eng="Proxy Server" jpn="プロキシーサーバー"
						registrytype="integer" dependskey="proxyflag" registrybytesize="4" default="0">
					<desc eng="Sets whether to use a proxy server."
						  jpn="プロキシーサーバーを使用するかどうかを設定します。"/>
					<selection>
						<option value="0" eng="Do Not Use" jpn="使用しない"/>
						<option value="1" eng="Use" jpn="使用する"/>
					</selection>
				</entry>

				<!-- In case of "Proxy Server = Use" : 「プロキシーサーバー = 使用する」の場合 -->
				<set dependson="proxyflag" conditionmin="1" conditionmax="1">

					<!-- Proxy Server : プロキシーサーバーアドレス -->
					<entry key="CONFIG/NET/#/APP/http_proxy_server" eng="Proxy Server Address" jpn="プロキシーサーバーアドレス"
							registrytype="string" registrybytesize="256" default="">
						<desc eng=""
							  jpn=""/>
						<textual minlen="1" maxlen="255" encoding="ascii"/>
					</entry>

					<!-- Proxy Server Port Number : プロキシーサーバーポート番号 -->
					<entry key="CONFIG/NET/#/APP/http_proxy_port" eng="Proxy Server Port Number" jpn="プロキシーサーバーポート番号"
							registrytype="integer" registrybytesize="4" default="8080">
						<desc eng=""
							  jpn=""/>
						<intvalue minvalue="1" maxvalue="65535"/>

					</entry>
				</set>

			</config>
		</group>

	    <set dependson="internalhost" conditionmin="1" conditionmax="1">

		<!-- Bluetooth(R) Settings : Bluetooth(R)設定 -->
		<group eng="Bluetooth(R) Settings" jpn="Bluetooth(R)設定">
			<desc eng="Adjusts settings for the Bluetooth(R) devices." jpn="Bluetooth(R)機器に関する設定です。"/>

			<!-- Bluetooth(R) Feature : Bluetooth(R)機能 -->
			<entry key="CONFIG/BT/bt_enable" eng="Bluetooth(R) Feature" jpn="Bluetooth(R)機能"
					registrytype="integer" registrybytesize="4" >
				<desc eng="If you set this option to [On], you will be able to use the Bluetooth(R) feature of this system (DevKit)."
					  jpn="[入]にすると、本機(開発機)のBluetooth(R)機能が使えるようになります。"/>
				<selection>
					<option value="0" eng="Off" jpn="切"/>
					<option value="1" eng="On" jpn="入"/>
				</selection>
			</entry>

			<!-- Prioritize the microphone of the Bluetooth(R) device : Bluetooth(R)機器のマイクを優先する -->
			<entry key="CONFIG/BT/bt_mic" eng="Prioritize the microphone of the Bluetooth(R) device" jpn="Bluetooth(R)機器のマイクを優先する"
					registrytype="integer" registrybytesize="4" >
				<desc eng="If you set this option to [On], the system software will prioritize the microphone of the Bluetooth(R) device over that of this system (DevKit)."
					  jpn="[入]にすると、Bluetooth(R)機器のマイクを本機(開発機)のマイクに優先して使用します。"/>
				<selection>
					<option value="0" eng="Off" jpn="切"/>
					<option value="1" eng="On" jpn="入"/>
				</selection>
			</entry>

		</group>

		</set>

	</group>

	<!-- PlayStation(R)Network : PlayStation(R)Network -->
	<group eng="PlayStation(R)Network" jpn="PlayStation(R)Network">
		<desc eng="Adjusts settings for PlayStation(R)Network." jpn="PlayStation(R)Networkに関する設定です。"/>

		<!-- NP Feature : NP機能 -->
		<entry key="CONFIG/NP/enable_np" eng="NP Feature" jpn="NP機能"
				registrytype="integer" registrybytesize="4" dependson="internalhost" conditionmin="1" conditionmax="1" norebootneeded="true">
			<desc eng="Sets whether to enable the NP feature. If you set this option to [On], the system automatically attempts sign-in in situations when you need to sign in."
				  jpn="NP機能を有効にするかどうかを設定します。[入]にすると、サインインが必要な場面で自動的にサインインします。"/>
			<selection>
				<option value="0" eng="Off" jpn="切"/>
				<option value="1" eng="On" jpn="入"/>
			</selection>
		</entry>

		<!-- NP Account ID -->
		<entry key="CONFIG/NP/account_id" eng="NP Account ID"
				registrytype="binary" registrybytesize="8" dependson="internalhost" conditionmin="1" conditionmax="1" norebootneeded="true">
			<desc eng=""
				  jpn=""/>
			<binary/>
		</entry>

		<!-- Sign-In ID (E-mail Address) : サインインID (Eメールアドレス) -->
		<entry key="CONFIG/NP/login_id" eng="Sign-In ID (E-mail Address)" jpn="サインインID (Eメールアドレス)"
				registrytype="string" registrybytesize="65" norebootneeded="true">
			<desc eng=""
				  jpn=""/>
			<textual minlen="0" maxlen="64" encoding="ascii"/>
		</entry>

		<!-- Sign-In Password : サインインパスワード -->
		<entry key="CONFIG/NP/password" eng="Sign-In Password" jpn="サインインパスワード"
				registrytype="string" registrybytesize="31" norebootneeded="true">
			<desc eng=""
				  jpn=""/>
			<password minlen="0" maxlen="30"/>
		</entry>

		<!-- Existence of Sub Account : サブアカウントの存在 -->
		<entry key="CONFIG/NP/has_subaccount" eng="Existence of Sub Account" jpn="サブアカウントの存在"
				registrytype="integer" registrybytesize="4" dependson="internalhost" conditionmin="1" conditionmax="1" norebootneeded="true">
			<desc eng="Sets whether the account that has been bound has a sub account."
				  jpn="バインドされているアカウントがサブアカウントを持っているかどうかを設定します。"/>
			<selection>
				<option value="0" eng="With No Sub Account" jpn="サブアカウントなし"/>
				<option value="1" eng="With Sub Account" jpn="サブアカウントあり"/>
			</selection>
		</entry>

		<!-- Country/Region of Residence : お住まいの国／地域 -->
		<entry key="CONFIG/NP/country" eng="Country/Region of Residence" jpn="お住まいの国／地域"
				registrytype="string" registrybytesize="3" dependson="internalhost" conditionmin="1" conditionmax="1" norebootneeded="true">
			<desc eng="Sets the contry code for PlayStation(R)Network account. The code must comply with ISO 3166-1."
				  jpn="PlayStation(R)Networkアカウント用の国コードを設定します。ISO 3166-1に準拠した国コードを入力してください。"/>
