<registry target="psp2" version = "3">

	<!-- Boot Parameters : ブートパラメータ -->
	<group eng="Boot Parameters" jpn="ブートパラメータ">
		<desc eng="Adjusts boot parameters applied to this system (DevKit) at boot time." jpn="本機(開発機)起動時に適用されるブートパラメータに関する設定です。" />

		<!-- Release Check Mode : リリースチェックモード -->
		<entry key="bootparam:/development_mode"
			   eng="Release Check Mode"
			   jpn="リリースチェックモード"
				 dependskey="rcm" registrytype="integer" registrybytesize="4" forcerefresh="true">
			<desc eng="If you set this option to [Release Mode], programs will run on almost the same environment as the actual system (console). In this mode, debugging and accessing to files on the host PC will be impossible.&#xa;This setting will be exclusively executed."
				  jpn="[リリースモード]にすると、実機と同等な環境でプログラムを実行します。リリースモードでは、デバッグやホスト上のファイルへのアクセスができません。&#xa;この設定は単独で実行されます。" />
			<applyimmediate eng="This setting will be exclusively executed. Any settings already set will be ignored.&#xa;Are you sure you want to continue?"
							jpn="この設定は単独で実行されます。すでに編集された他の設定値は無視されます。&#xa;本当によろしいですか？"/>
			<selection>
				<option value="0" eng="Release Mode" jpn="リリースモード" />
				<option value="1" eng="Development Mode" jpn="開発モード" />
			</selection>
		</entry>

		<!-- Memory Size : メモリサイズ -->
		<entry key="bootparam:/memory_size_switch" eng="Memory Size" jpn="メモリサイズ"
		   registrytype="integer" registrybytesize="4" dependson="rcm" conditionmin="1" conditionmax="1">
			<desc eng="If you set this option to [Console Size], programs will run under the actual system's (console's) memory size."
				  jpn="[実機サイズ]にすると、実機と同じメモリサイズでプログラムを実行します。" />
			<selection>
				<option value="0" eng="Console Size" jpn="実機サイズ" />
				<option value="1" eng="Development Tool Size" jpn="開発機サイズ" />
			</selection>
		</entry>
	</group>

	<set dependson="rcm" conditionmin="1" conditionmax="1">

		<!-- Development Environment Settings : 開発環境設定 -->
		<group eng="Development Environment Settings" jpn="開発環境設定">
			<desc eng="Adjusts settings for the development environment." jpn="開発環境全般に関する設定です。"/>

			<!-- System Name : 本体名称 -->
			<entry key="registry:/DEVENV/HOST/devkitname" eng="System Name" jpn="本体名称"
					registrytype="string" registrybytesize="128" >
				<desc eng="Sets the name for this system (DevKit). You can use the name to distinguish multiple DevKits on a network."
					  jpn="本機(開発機)の名前を設定します。ネットワーク上で複数の開発機を区別するときなどに使われます。"/>
				<textual minlen="0" maxlen="31" encoding="utf8"/>
			</entry>

			<!-- HDMI Video Output Settings : HDMI 映像出力設定 -->
			<entry key="registry:/DEVENV/TOOL/hdmi_video_mode" eng="HDMI Video Output Settings" jpn="HDMI 映像出力設定"
					registrytype="integer" registrybytesize="4" norebootneeded="true">
				<desc eng="Sets the resolution and scaling type of the HDMI video output."
					  jpn="HDMI 映像出力の解像度と拡大/縮小方法を設定します。"/>
				<selection>
					<option value="8" eng="Resolution : 480p, Scaling : Full" jpn="解像度 : 480p, 拡大/縮小 : フル"/>
					<option value="32" eng="Resolution : 720p, Scaling : Full" jpn="解像度 : 720p, 拡大/縮小 : フル"/>
					<option value="65568" eng="Resolution : 720p, Scaling : Real" jpn="解像度 : 720p, 拡大/縮小 : リアル"/>
				</selection>
			</entry>

		</group>

		<!-- Network : ネットワーク -->
		<group eng="Network" jpn="ネットワーク">
			<desc eng=" " jpn=" "/>

			<!-- Wi-Fi Settings : Wi-Fi設定 -->
			<group eng="Wi-Fi Settings" jpn="Wi-Fi設定">
				<desc eng="Adjusts settings for the Wi-Fi connection." jpn="Wi-Fi接続に関する設定です。"/>

				<!-- Show Imposed Scan List : スキャンリストインポーズ表示 -->
				<entry key="registry:/CONFIG/NET/enable_impose_scanlist" eng="Show Imposed Scan List" jpn="スキャンリストインポーズ表示"
						registrytype="integer" registrybytesize="4">
					<desc eng="If you set this option to [On], an imposed scan list will be displayed."
						  jpn="[入]にすると、インポーズでスキャンリストが表示されます。"/>
					<selection>
						<option value="0" eng="Off" jpn="切"/>
						<option value="1" eng="On" jpn="入"/>
					</selection>
				</entry>

				<!-- Connection #01-30 : Wi-Fiインターネット接続 #01-30 -->
				<config keymin="1" keymax="30" keysigfig="2" validkey="registry:/CONFIG/NET/#/COMMON/conf_flag"
						validkeyvalue="1" validkeyregistrytype="integer" eng="Wi-Fi Internet Connection" jpn="Wi-Fiインターネット接続">
					<desc eng="Adjusts settings for the Wi-Fi Internet connection."
						  jpn="Wi-Fiインターネット接続に関する設定です。"/>

					<!-- Connection Name : 接続名 -->
					<entry key="registry:/CONFIG/NET/#/COMMON/conf_name" eng="Connection Name" jpn="接続名"
							registrytype="string" registrybytesize="65" default="">
						<desc eng="Sets the name of the Internet connection setting."
							  jpn="インターネット接続の接続名を設定します。"/>
						<textual minlen="1" maxlen="16" encoding="utf8"/>
					</entry>

					<!-- Automatic Connect : 自動接続 -->
					<entry key="registry:/CONFIG/NET/#/COMMON/enable_auto_connect" eng="Auto Connect" jpn="自動接続"
							registrytype="integer" registrybytesize="4" default="1">
						<desc eng="If you set this option to [On], automatic connection is made when a connection is found."
							  jpn="[入]にすると、この接続が見つかったときに自動で接続します。"/>
						<selection>
							<option value="0" eng="Off" jpn="切"/>
							<option value="1" eng="On" jpn="入"/>
						</selection>
					</entry>

					<!-- MTU -->
					<entry key="registry:/CONFIG/NET/#/COMMON/mtu" eng="MTU"
							registrytype="integer" registrybytesize="4" default="0">
						<desc eng="Sets the MTU value to use when transmitting data. Set a number from the ranges 576-1500. Use 0 for automatic setup. "
							  jpn="データ通信時に使用するMTUの値を設定します。576から1500の値を設定します。自動設定にするには0を入力してください。"/>
						<intvalue minvalue="576" maxvalue="1500" rangeexception="0"/>
					</entry>

					<!-- Networking Device : 接続デバイス -->
					<entry key="registry:/CONFIG/NET/#/COMMON/device" eng="Networking Device" jpn="接続デバイス"
							registrytype="integer" registrybytesize="4" default="0">
						<desc eng="Sets the device for the Internet connection."
							  jpn="インターネット接続に使用するデバイスを設定します。"/>
						<selection>
							<option value="0" eng="Wireless (currently unsupported)" jpn="無線 (未サポート)"/>
							<option value="1" eng="Wired Connection" jpn="有線"/>
						</selection>
					</entry>

					<!-- IP Address Setting : IPアドレス設定 -->
					<entry key="registry:/CONFIG/NET/#/IP/ip_config" eng="IP Address Setting" jpn="IPアドレス設定"
							registrytype="integer" registrybytesize="4" dependskey="ipconfig" default="0">
						<desc eng="Select how to set the IP address."
							  jpn="IPアドレスの設定方法を選択します。"/>
						<selection>
							<option value="0" eng="Automatic" jpn="自動取得"/>
							<option value="1" eng="Manual" jpn="手動"/>
							<option value="2" eng="PPPoE" jpn="PPPoE"/>
						</selection>
					</entry>

					<!-- In case of "IP Address Setting = Automatic" : 「IPアドレス設定 = 自動取得」の場合 -->
					<set dependson="ipconfig" conditionmin="0" conditionmax="0">

						<!-- DHCP Host Name : DHCPホスト名 -->
						<entry key="registry:/CONFIG/NET/#/IP/dhcp_hostname" eng="DHCP Host Name" jpn="DHCPホスト名"
								registrytype="string" registrybytesize="256" default="">
							<desc eng="Sets the DHCP host name. In most cases this setting does not need to be changed."
								  jpn="DHCPホスト名を設定します。通常は設定する必要はありません。"/>
							<textual minlen="0" maxlen="255" encoding="ascii"/>
						</entry>

						<!-- DNS Setting : DNS設定 -->
						<entry key="registry:/CONFIG/NET/#/IP/dns_flag" eng="DNS Setting" jpn="DNS設定"
								registrytype="integer" registrybytesize="4" dependskey="dnsflag1" default="0">
							<desc eng="Select how to set the DNS server addresses."
								  jpn="DNSサーバーのアドレスの設定方法を選択します。"/>
							<selection>
								<option value="0" eng="Automatic" jpn="自動取得"/>
								<option value="1" eng="Manual" jpn="手動"/>
							</selection>
						</entry>

						<!-- In case of "DNS Setting = Manual" : 「DNS設定 = 手動」の場合 -->
						<set dependson="dnsflag1" conditionmin="1" conditionmax="1">

							<!-- Primary DNS : プライマリーDNS -->
							<entry key="registry:/CONFIG/NET/#/IP/primary_dns" eng="Primary DNS" jpn="プライマリーDNS"
									registrytype="string" registrybytesize="16" default="">
								<desc eng=" "
									  jpn=" "/>
								<ipaddress/>
							</entry>

							<!-- Secondary DNS : セカンダリーDNS -->
							<entry key="registry:/CONFIG/NET/#/IP/secondary_dns" eng="Secondary DNS" jpn="セカンダリーDNS"
									registrytype="string" registrybytesize="16" default="">
								<desc eng=" "
									  jpn=" "/>
								<ipaddress/>
							</entry>

						</set>
					</set>

					<!-- In case of "IP Address Setting = Manual" : 「IPアドレス設定 = 手動」の場合 -->
					<set dependson="ipconfig" conditionmin="1" conditionmax="1">

						<!-- IP Address : IPアドレス -->
						<entry key="registry:/CONFIG/NET/#/IP/ip_address" eng="IP Address" jpn="IPアドレス"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=" "
								  jpn=" "/>
							<ipaddress/>
						</entry>

						<!-- Subnet Mask : サブネットマスク -->
						<entry key="registry:/CONFIG/NET/#/IP/netmask" eng="Subnet Mask" jpn="サブネットマスク"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=" "
								  jpn=" "/>
							<ipaddress/>
						</entry>

						<!-- Default Router : デフォルトルーター -->
						<entry key="registry:/CONFIG/NET/#/IP/default_route" eng="Default Router" jpn="デフォルトルーター"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=" "
								  jpn=" "/>
							<ipaddress/>
						</entry>

						<!-- Primary DNS : プライマリーDNS -->
						<entry key="registry:/CONFIG/NET/#/IP/primary_dns" eng="Primary DNS" jpn="プライマリーDNS"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=" "
								  jpn=" "/>
							<ipaddress/>
						</entry>

						<!-- Secondary DNS : セカンダリーDNS -->
						<entry key="registry:/CONFIG/NET/#/IP/secondary_dns" eng="Secondary DNS" jpn="セカンダリーDNS"
								registrytype="string" registrybytesize="16" default="">
							<desc eng=" "
								  jpn=" "/>
							<ipaddress/>
						</entry>

					</set>

					<!-- In case of "IP Address Setting = PPPoE" : 「IPアドレス設定 = PPPoE」の場合 -->
					<set dependson="ipconfig" conditionmin="2" conditionmax="2">

						<!-- PPPoE User Name : PPPoEユーザーID -->
						<entry key="registry:/CONFIG/NET/#/IP/auth_name" eng="PPPoE User Name" jpn="PPPoEユーザーID"
								registrytype="string" registrybytesize="128" default="">
							<desc eng=" "
								  jpn=" "/>
							<textual minlen="1" maxlen="127" encoding="ascii"/>
						</entry>

						<!-- PPPoE Password : PPPoEパスワード -->
						<entry key="registry:/CONFIG/NET/#/IP/auth_key" eng="PPPoE Password" jpn="PPPoEパスワード"
								registrytype="string" registrybytesize="128" default="">
							<desc eng=" "
								  jpn=" "/>
							<password minlen="0" maxlen="127"/>
						</entry>

						<!-- DNS Setting : DNS設定 -->
						<entry key="registry:/CONFIG/NET/#/IP/dns_flag" eng="DNS Setting" jpn="DNS設定"
								registrytype="integer" registrybytesize="4" dependskey="dnsflag2" default="0">
							<desc eng="Select how to set the DNS server addresses."
								  jpn="DNSサーバーのアドレスの設定方法を選択します。"/>
							<selection>
								<option value="0" eng="Automatic" jpn="自動取得"/>
								<option value="1" eng="Manual" jpn="手動"/>
							</selection>
						</entry>

						<!-- In case of "DNS Setting = Manual" : 「DNS設定 = 手動」の場合 -->
						<set dependson="dnsflag2" conditionmin="1" conditionmax="1">

							<!-- Primary DNS : プライマリーDNS -->
							<entry key="registry:/CONFIG/NET/#/IP/primary_dns" eng="Primary DNS" jpn="プライマリーDNS"
									registrytype="string" registrybytesize="16" default="">
								<desc eng=" "
									  jpn=" "/>
								<ipaddress/>
							</entry>

							<!-- Secondary DNS : セカンダリーDNS -->
							<entry key="registry:/CONFIG/NET/#/IP/secondary_dns" eng="Secondary DNS" jpn="セカンダリーDNS"
									registrytype="string" registrybytesize="16" default="">
								<desc eng=" "
									  jpn=" "/>
								<ipaddress/>
							</entry>

						</set>
					</set>

					<!-- Proxy Server : プロキシーサーバー -->
					<entry key="registry:/CONFIG/NET/#/APP/http_proxy_flag" eng="Proxy Server" jpn="プロキシーサーバー"
							registrytype="integer" dependskey="proxyflag" registrybytesize="4" default="0">
						<desc eng="Sets whether to use a proxy server."
							  jpn="プロキシーサーバーを使用するかどうかを設定します。"/>
						<selection>
							<option value="0" eng="Do Not Use" jpn="使用しない"/>
							<option value="1" eng="Use" jpn="使用する"/>
						</selection>
					</entry>

					<!-- In case of "Proxy Server = Use" : 「プロキシーサーバー = 使用する」の場合 -->
					<set dependson="proxyflag" conditionmin="1" conditionmax="1">

						<!-- Proxy Server : プロキシーサーバーアドレス -->
						<entry key="registry:/CONFIG/NET/#/APP/http_proxy_server" eng="Proxy Server Address" jpn="プロキシーサーバーアドレス"
								registrytype="string" registrybytesize="256" default="">
							<desc eng=" "
								  jpn=" "/>
							<textual minlen="1" maxlen="255" encoding="ascii"/>
						</entry>

						<!-- Proxy Server Port Number : プロキシーサーバーポート番号 -->
						<entry key="registry:/CONFIG/NET/#/APP/http_proxy_port" eng="Proxy Server Port Number" jpn="プロキシーサーバーポート番号"
								registrytype="integer" registrybytesize="4" default="8080">
							<desc eng=" "
								  jpn=" "/>
							<intvalue minvalue="1" maxvalue="65535"/>

						</entry>
					</set>

				</config>
			</group>

		</group>

		<!-- PlayStation(R)Network : PlayStation(R)Network -->
		<group eng="PlayStation(R)Network" jpn="PlayStation(R)Network">
			<desc eng="Adjusts settings for PlayStation(R)Network." jpn="PlayStation(R)Networkに関する設定です。"/>

			<!-- Sign-In ID (E-mail Address) : サインインID (Eメールアドレス) -->
			<entry key="registry:/CONFIG/NP/login_id" eng="Sign-In ID (E-mail Address)" jpn="サインインID (Eメールアドレス)"
					registrytype="string" registrybytesize="65" norebootneeded="true">
				<desc eng=" "
					  jpn=" "/>
				<textual minlen="0" maxlen="64" encoding="ascii"/>
			</entry>

			<!-- Sign-In Password : サインインパスワード -->
			<entry key="registry:/CONFIG/NP/password" eng="Sign-In Password" jpn="サインインパスワード"
					registrytype="string" registrybytesize="31" norebootneeded="true">
				<desc eng=" "
					  jpn=" "/>
				<password minlen="0" maxlen="30"/>
			</entry>

		</group>

		<!-- Location Data : 位置データ -->
		<group eng="Location Data" jpn="位置データ">
			<desc eng="Adjusts settings for location data." jpn="位置データに関する設定です。"/>

			<!-- Use Location Data : 位置データを利用する -->
			<entry key="registry:/CONFIG/LOCATION/enable_service" eng="Use Location Data" jpn="位置データを利用する"
					registrytype="integer" registrybytesize="4" norebootneeded="true">
				<desc eng="Sets whether to use location data. If you set this option to [Off], you will not be able to utilize location data in all applications. "
					  jpn="位置データを利用するかどうかを設定します。[切]にすると、すべてのアプリケーションにおいて位置データが利用できません。"/>
				<selection>
					<option value="0" eng="Off" jpn="切"/>
					<option value="1" eng="On" jpn="入"/>
				</selection>
			</entry>

		</group>

		<!-- Security : セキュリティー -->
		<group eng="Security" jpn="セキュリティー">
			<desc eng="Adjusts settings for parental controls and screen lock." jpn="ペアレンタルコントロールおよび画面ロックに関する設定です。"/>

			<group eng="Parental Controls" jpn="ペアレンタルコントロール">
				<desc eng="Adjusts settings for parental controls." jpn="ペアレンタルコントロールに関する設定です。"/>

				<!-- Content : コンテンツ -->
				<entry key="registry:/CONFIG/SECURITY/PARENTAL/content_start_control" eng="Content" jpn="コンテンツ"
						registrytype="integer" registrybytesize="4" norebootneeded="true">
					<desc eng="Sets the control level for starting an application or content (such as video, etc.) that has parental control restrictions.&#xa;The lower the level, the tighter the control."
						  jpn="年齢制限があるアプリケーションやコンテンツ(ビデオなど)の開始制限レベルを設定します。&#xa;レベルの数値が小さいほど、制限が厳しくなります。"/>
					<selection>
						<option value="0" eng="Off" jpn="切"/>
						<option value="11" eng="Level 11" jpn="レベル 11"/>
						<option value="10" eng="Level 10" jpn="レベル 10"/>
						<option value="9" eng="Level 9" jpn="レベル 9"/>
						<option value="8" eng="Level 8" jpn="レベル 8"/>
						<option value="7" eng="Level 7" jpn="レベル 7"/>
						<option value="6" eng="Level 6" jpn="レベル 6"/>
						<option value="5" eng="Level 5" jpn="レベル 5"/>
						<option value="4" eng="Level 4" jpn="レベル 4"/>
						<option value="3" eng="Level 3" jpn="レベル 3"/>
						<option value="2" eng="Level 2" jpn="レベル 2"/>
						<option value="1" eng="Level 1" jpn="レベル 1"/>
					</selection>
				</entry>

				<!-- Browser : ブラウザー -->
				<entry key="registry:/CONFIG/SECURITY/PARENTAL/browser_start_control" eng="Browser" jpn="ブラウザー"
						registrytype="integer" registrybytesize="4" norebootneeded="true">
					<desc eng="Sets whether to allow to use the browser feature."
						  jpn="ブラウザー機能の利用を許可するかどうかを設定します。"/>
					<selection>
						<option value="0" eng="Allow" jpn="許可する"/>
						<option value="1" eng="Do Not Allow" jpn="許可しない"/>
					</selection>
				</entry>

				<!-- Location Data : 位置データ -->
				<entry key="registry:/CONFIG/LOCATION/enable_parental" eng="Location Data" jpn="位置データ"
						registrytype="integer" registrybytesize="4" norebootneeded="true">
					<desc eng="Sets whether to allow to use location data."
						  jpn="位置データの利用を許可するかどうかを設定します。"/>
					<selection>
						<option value="1" eng="Allow" jpn="許可する"/>
						<option value="0" eng="Do Not Allow" jpn="許可しない"/>
					</selection>
				</entry>

				<!-- Change Passcode : パスコードを変更する -->
				<entry key="registry:/CONFIG/SECURITY/PARENTAL/passcode" eng="Change Passcode" jpn="パスコードを変更する"
						registrytype="string" registrybytesize="5" norebootneeded="true">
					<desc eng="Change the 4-digit passcode required to change the settings for parental controls. To reset the passcode, leave it blank.&#xa;The passcode will not have valid protection. Do not set the passcode using the information you do not wish to disclose."
						  jpn="ペアレンタルコントールの設定を変更するための4桁のパスコードを変更します。パスコードを未設定状態にするには、空白にしてください。&#xa;パスコードには十分な保護がされません。知られたくない情報をパスコードに設定しないでください。"/>
					<password minlen="4" maxlen="4" rangeexception="0"/>
				</entry>

			</group>

			<group eng="Screen Lock" jpn="画面ロック">
				<desc eng="Adjusts settings for screen lock." jpn="画面ロックに関する設定です。"/>

				<!-- Change Passcode : パスコードを変更する -->
				<entry key="registry:/CONFIG/SECURITY/SCREEN_LOCK/passcode" eng="Change Passcode" jpn="パスコードを変更する"
						registrytype="string" registrybytesize="5" norebootneeded="true">
					<desc eng="Change the 4-digit passcode required to unlock the screen. To disable the screen lock feature, leave it blank.&#xa;The passcode will not have valid protection. Do not set the passcode using the information you do not wish to disclose."
						  jpn="画面ロックを解除するための4桁のパスコードを変更します。画面ロック機能を無効にするには、空白にしてください。&#xa;パスコードには十分な保護がされません。知られたくない情報をパスコードに設定しないでください。"/>
					<password minlen="4" maxlen="4" rangeexception="0"/>
				</entry>

			</group>

		</group>

		<!-- Date & Time : 日付＆時刻 -->
		<group eng="Date &amp; Time" jpn="日付＆時刻">
			<desc eng="Adjusts date and time settings." jpn="日付と時刻に関する設定です。"/>

			<!-- Time Zone : タイムゾーン -->
			<entry key="registry:/CONFIG/DATE/time_zone" eng="Time Zone" jpn="タイムゾーン"
					registrytype="integer" registrybytesize="4" >
				<desc eng="Sets the time zone to be used as the basis for time settings."
					  jpn="時刻の基準となるタイムゾーンを設定します。"/>
				<selection>
					<option value="0" eng="GMT-11:00 Samoa Islands" jpn="GMT-11:00 サモア諸島"/>
					<option value="1" eng="GMT-11:00 Midway Islands" jpn="GMT-11:00 ミッドウェー諸島"/>
					<option value="2" eng="GMT-10:00 Hawaii" jpn="GMT-10:00 ハワイ"/>
