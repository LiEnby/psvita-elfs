<system_settings version="1.0" plugin="psn_settings_plugin">
  <!-- サブアカウント設定 -->
  <setting_list id="change_subaccount"
								title=""
								style="edit"
								navi_left="cancel"
								navi_center="policy"
								navi_right="send">
		<list id="chat" title="msg_chat">
			<list_item id="chat_permit" title="msg_permit" value="permit"/>
			<list_item id="chat_deny" title="msg_deny" value="deny"/>
		</list>
		<list id="content_restriction"  title="msg_content_restriction">
			<list_item id="cr_permit" title="msg_permit" value="permit"/>
			<list_item id="cr_deny" title="msg_deny" value="deny"/>
		</list>
		<list id="purchase_limit"  title="msg_purchase_limit"/>
		<link id="change_subaccount_password" title="msg_change_password"
					file="psn_settings_change_password.xml"/>
  </setting_list>
</system_settings>
