<system_settings version="1.0" plugin="psn_settings_plugin">
  <!-- パスワード設定 -->
  <setting_list id="change_password"
								style="edit"
								title="msg_change_subaccount_pw"
								navi_left="cancel"
								navi_center="policies"
								navi_right="send">
		<label id="signin_id" title="msg_signin_id"/>
		<text_field id="password1" title="msg_pw"
								max_length="30"
								min_length="8"
								password="on"/>
		<text_field id="password2" title="msg_password_reenter"
								max_length="30"
								min_length="8"
								password="on"/>
  </setting_list>
</system_settings>
